<h1>Display your Pirate here</h1>
<div class="pirate">
  <h2>Name: <%= @pirate.name %></h2><br>
  <h3>Weight: <%= @pirate.weight %></h3>
  <h3>Height: <%= @pirate.height %></h3>
</div><br>

<h1>Courses</h1>
<% @courses.each do |course| %>
  <div class="course">
    <p>Name: <%= course.name %></p><br>
    <p>Topic: <%= course.topic %></p><br>
  </div><br>
<% end %>

